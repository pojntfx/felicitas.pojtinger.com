{
  "name": "@pojntfx/site",
  "version": "0.0.1-alpha.0",
  "description": "Felix Pojtinger's personal site",
  "author": "Felix Pojtinger <felix@pojtinger.com>",
  "license": "AGPL-3.0",
  "homepage": "https://gitlab.com/pojntfx/projects#README",
  "scripts": {
    "dev": "run-p app.dev--watch",
    "build": "run-p app.prod::build web.prod::build",
    "start": "pm2-runtime start npm -- run web.prod::start",
    "install--binary": "sudo install bin/pojntfx-site /usr/bin",
    "test": "run-p app.prod::test web.prod::test",
    "deploy": "exoframe",
    "dev--watch": "run-p dev",
    "build--watch": "run-p app.prod::build--watch web.prod::build--watch",
    "start--watch": "run-p app.prod::start--watch web.prod::start--watch",
    "test--watch": "run-p app.prod::test--watch web.prod::test--watch",
    "app.prod": "run-s app.prod::build app.prod::start",
    "app.prod::build": "gatsby build",
    "app.prod::start": "gatsby develop",
    "app.prod::test": "echo \"Error: no test specified\" && exit 1",
    "app.prod::build--watch": "nodemon -x 'run-s app.prod::build'",
    "app.prod::start--watch": "run-s app.prod::start",
    "app.prod::test--watch": "run-s app.prod::test",
    "app.dev": "run-s app.dev:build app.dev::start",
    "app.dev::build": "run-s app.prod::build",
    "app.dev::start": "run-s app.prod::start",
    "app.dev::test": "run-s app.prod::test",
    "app.dev--watch": "gatsby develop",
    "app.dev::build--watch": "run-s app.prod::build--watch",
    "app.dev::start--watch": "run-s app.prod::start--watch",
    "app.dev::test--watch": "run-s app.prod::test--watch",
    "web.prod": "run-s web.prod::build web.prod::start",
    "web.prod::build": "tsc --outDir dist/ src/start.ts",
    "web.prod::start": "node dist/start.js",
    "web.prod::test": "echo \"Error: no test specified\" && exit 1",
    "web.prod--watch": "run-s web.prod::build--watch web.prod::start--watch",
    "web.prod::build--watch": "tsc -w --outDir dist/ src/start.ts",
    "web.prod::start--watch": "nodemon -x 'run-s web.prod::start'",
    "web.prod::test--watch": "nodemon -x 'run-s web.prod::test'",
    "web.prod--binary": "run-s web.prod::build--binary web.prod::start--binary",
    "web.prod::build--binary": "mkdir -p bin/ && pkg package.json -t node10-linux-x64 -o bin/pojntfx-site",
    "web.prod::start--binary": "chmod +x bin/pojntfx-site && bin/pojntfx-site",
    "web.prod::build--watch--binary": "nodemon -x 'run-s web.prod::build--binary'",
    "web.prod::start--watch--binary": "nodemon -x 'run-s web.prod::start--binary'",
    "web.dev": "run-s web.dev::build web.dev::start",
    "web.dev::build": "run-s web.prod::build",
    "web.dev::start": "ts-node --files -P tsconfig.json src/start.ts",
    "web.dev::test": "run-s web.prod::test",
    "web.dev--watch": "run-p web.dev::build--watch web.dev::start--watch",
    "web.dev::build--watch": "run-s web.prod::build--watch",
    "web.dev::start--watch": "nodemon -x 'run-s web.dev::start'",
    "web.dev::test--watch": "run-s web.prod::test--watch",
    "prepare": "run-p build"
  },
  "dependencies": {
    "@libresat/frontend-components": "^0.0.1-75",
    "@mdx-js/mdx": "^0.15.0",
    "@mdx-js/tag": "^0.15.0",
    "copy-webpack-plugin": "^4.6.0",
    "core-js": "^2.6.1",
    "emotion": "^9.2.12",
    "emotion-server": "^9.2.12",
    "express": "^4.16.4",
    "gatsby": "^2.0.89",
    "gatsby-link": "^2.0.7",
    "gatsby-mdx": "0.2.0",
    "gatsby-plugin-emotion": "^2.0.7",
    "gatsby-plugin-manifest": "^2.0.13",
    "gatsby-plugin-offline": "^2.0.21",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-typescript": "^2.0.3",
    "gatsby-source-filesystem": "^2.0.13",
    "gatsby-transformer-yaml": "^2.1.7",
    "html-loader": "^0.5.5",
    "markdown-loader": "^4.0.0",
    "pm2": "^3.2.5",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-emotion": "^9.2.12",
    "react-helmet": "^5.2.0",
    "react-masonry-component": "^6.2.1",
    "reactjs-coverflow": "^2.1.3",
    "semantic-ui-react": "^0.83.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/react": "^16.7.18",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.5",
    "babel-core": "^6.26.3",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.11.0",
    "pkg": "^4.3.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3"
  },
  "main": "public/start.js",
  "source": "src/start.js",
  "bin": "public/start.js",
  "pkg": {
    "assets": [
      "public/**/*"
    ],
    "scripts": "public/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/pojntfx/projects.git"
  },
  "bugs": {
    "url": "https://gitlab.com/pojntfx/projects/issues"
  },
  "keywords": [
    "chinese"
  ],
  "publishConfig": {
    "access": "public"
  }
}
